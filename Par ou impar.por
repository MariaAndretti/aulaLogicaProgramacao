programa {
  funcao inicio() {
    
    //Peça um numero inteiro ao usuario e diga se ele é impar ou par
   
    inteiro numero // Declarei a variavel

    escreva("Informe um número: ")
    leia(numero)

    se(numero % 2 == 0){
      escreva("Número par: ", numero)}

    senao{
       escreva("número impar: ", numero)}

 

  }
}
